{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "jogo.html", "use": "@vercel/static" },
    { "src": "creditos.html", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/jogo", "dest": "/jogo.html" },
    { "src": "/creditos", "dest": "/creditos.html" }
  ]
}
